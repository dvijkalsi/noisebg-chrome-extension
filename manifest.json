{
  "manifest_version": 2,
  "name": "noisebg",
  "description": "add a bg to google.com",
  "version": "1.0.0",
  "icons": {
    "16": "/images/Untitled_design_16.png",
    "48": "/images/Untitled_design_48.png",
    "128": "/images/Untitled_design_128.png"
  },
  "page_action": {
    "default_icon": {
      "16": "/images/Untitled_design_16.png",
      "48": "/images/Untitled_design_48.png",
      "128": "/images/Untitled_design_128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "css": ["main.css"]
    }
  ]
}
